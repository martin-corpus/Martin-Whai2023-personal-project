{
  "name": "boilerplate-react",
  "version": "0.2.0",
  "description": "React!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev-academy-challenges/react-boilerplate.git"
  },
  "main": "server/index.ts",
  "scripts": {
    "start": "node dist/server/index.js",
    "test": "vitest",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "knex": "knex --knexfile server/db/knexfile.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "format": "prettier -w '**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "author": "Dev Academy Aotearoa",
  "license": "ISC",
  "devDependencies": {
    "@devacademy/eslint-config": "^1.8.0",
    "@tanstack/react-query": "^4.29.12",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.35",
    "@types/jquery": "^3.5.16",
    "@types/knex": "^0.16.1",
    "@types/node": "^18.16.16",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-router": "^5.1.20",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "jsdom": "^22.1.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-router": "^6.12.1",
    "react-router-dom": "^6.4.2",
    "superagent": "^8.0.9",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "browserslist": "> 2%, not dead",
  "dependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "knex": "^2.4.2",
    "react-icons": "^4.9.0",
    "sqlite3": "^5.1.6"
  },
  "bugs": {
    "url": "https://github.com/dev-academy-challenges/react-boilerplate/issues"
  },
  "homepage": "https://github.com/dev-academy-challenges/react-boilerplate#readme",
  "keywords": []
}
